<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
    <!ENTITY ex "http://example.org/ontology#">
]>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:ex="http://example.org/ontology#">

    <rdf:Description rdf:about="http://example.org/ontology#Person">
        <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    </rdf:Description>

    <rdf:Description rdf:about="http://example.org/ontology#John">
        <rdf:type rdf:resource="http://example.org/ontology#Person"/>
        <ex:age rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">30</ex:age>
    </rdf:Description>

    <rdf:Description rdf:about="http://example.org/ontology#Mary">
        <rdf:type rdf:resource="http://example.org/ontology#Person"/>
        <ex:age rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">25</ex:age>
    </rdf:Description>

</rdf:RDF>
