Prefix: wine-ont: <http://www.semanticweb.org/wine-ontology#>
Prefix: rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
Prefix: rdfs: <http://www.w3.org/2000/01/rdf-schema#>

Ontology: <http://www.semanticweb.org/wine-ontology>

# Concepts
Class: wine-ont:Wine
    SubClassOf: rdf:Resource
    EquivalentTo: wine-ont:RedWine or wine-ont:WhiteWine or wine-ont:RoseWine

Class: wine-ont:RedWine
    SubClassOf: wine-ont:Wine

Class: wine-ont:WhiteWine
    SubClassOf: wine-ont:Wine

Class: wine-ont:RoseWine
    SubClassOf: wine-ont:Wine

# Properties
ObjectProperty: wine-ont:madeFrom
    Domain: wine-ont:Wine
    Range: owl:Thing
    SubPropertyOf: rdf:type

DataProperty: wine-ont:color
    Domain: wine-ont:Wine
    Range: xsd:string

DataProperty: wine-ont:flavor
    Domain: wine-ont:Wine
    Range: xsd:string

DataProperty: wine-ont:region
    Domain: wine-ont:Wine
    Range: xsd:string

# Individuals
Individual: wine-ont:Merlot
    Types: wine-ont:RedWine
    Facts: wine-ont:flavor "Fruity and soft"; wine-ont:region "Bordeaux, France"; wine-ont:madeFrom wine-ont:Grapes
