<rdf:RDF xmlns="http://example.com/ontology#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#">

  <owl:Ontology rdf:about="http://example.com/ontology"/>

  <owl:Class rdf:about="#Animal">
    <rdfs:subClassOf rdf:resource="#LivingThing"/>
  </owl:Class>

  <owl:Class rdf:about="#LivingThing"/>

  <owl:Class rdf:about="#Plant">
    <rdfs:subClassOf rdf:resource="#LivingThing"/>
  </owl:Class>

  <owl:ObjectProperty rdf:about="#eats">
    <rdfs:domain rdf:resource="#Animal"/>
    <rdfs:range rdf:resource="#Plant"/>
  </owl:ObjectProperty>

</rdf:RDF>
